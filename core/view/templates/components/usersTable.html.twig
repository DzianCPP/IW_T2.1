<table class="table table-sm table-hover" id="all-users-table">
        <tr>
            <th scope="col"><input type="checkbox" id="check-all"></th>
            <th scope="col">ID</th>
            <th scope="col">E-mail</th>
            <th scope="col">Full Name</th>
            <th scope="col">Gender</th>
            <th scope="col">Status</th>
            <th scope="col"></th>
        </tr>
    {% for user in allUsers %}
        <tr>
            <td><input name="select-user" type="checkbox" value="{{user.userID}}"></td>
            <td>{{user.userID}}</td>
            <td>{{user.email}}</td>
            <td>{{user.fullName}}</td>
            <td>
                {% for key, value in GENDERS %}
                {% if key == user.gender %}
                {{ value }}
                {% endif %}
                {% endfor %}
            </td>
            <td>
                {% for key, value in STATUSES %}
                {% if key == user.status %}
                {{ value }}
                {% endif %}
                {% endfor %}
            </td>
            <td>
                <div class="btn-group-vertical">
                    <a class="btn btn-success" href="/user/edit/{{user.userID}}"">Edit</a>
                    <a class="btn btn-dark" id="{{user.userID}}" onclick="sendDeleteRequest(this.id)">
                        Delete</a>
                </div>
            </td>
        </tr>
    {% endfor %}

</table>